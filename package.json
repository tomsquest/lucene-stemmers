{
  "name": "lucene-stemmers",
  "version": "1.1.0",
  "description": "Stem words like with Lucene",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "prettier:fix": "prettier --write --no-config '*.json' 'src/**/*.{ts,json}'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsquest/lucene-stemmers.git"
  },
  "keywords": [
    "lucene",
    "stem",
    "stemming",
    "stemmer",
    "stemmers",
    "search"
  ],
  "author": "Thomas Queste <tom@tomsquest.com> (http://www.tomsquest.com/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tomsquest/lucene-stemmers/issues"
  },
  "homepage": "https://github.com/tomsquest/lucene-stemmers#readme",
  "jest": {
    "notify": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testTimeout": 60000,
    "rootDir": "src"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  }
}
